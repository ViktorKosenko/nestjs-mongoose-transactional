{
  "name": "nestjs-mongoose-transactional",
  "version": "0.1.5",
  "description": "A transactional decorator implementation is provided for Nestjs Mongoose Transactions.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint \"lib/**/*.ts\" --fix",
    "format": "prettier \"lib/**/*.ts\" --write",
    "build": "rm -rf dist && tsc",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prerelease": "npm run build",
    "release": "release-it",
    "test:e2e": "jest --config ./tests/jest-e2e.json --runInBand",
    "test:e2e:dev": "jest --config ./tests/jest-e2e.json --runInBand --watch",
    "prepare": "husky install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ViktorKosenko/nestjs-mongoose-transactional.git"
  },
  "author": "Viktor Kosenko (viktorkosenkouk@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ViktorKosenko/nestjs-mongoose-transactional/issues"
  },
  "homepage": "https://github.com/ViktorKosenko/nestjs-mongoose-transactional#readme",
  "devDependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/mongoose": "^9.2.0",
    "@types/jest": "29.5.3",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "eslint": "8.46.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "8.0.3",
    "jest": "29.6.2",
    "lint-staged": "13.2.3",
    "mongoose": "^6.11.3",
    "prettier": "3.0.1",
    "reflect-metadata": "0.1.13",
    "release-it": "16.1.4",
    "rxjs": "7.8.1",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "4.5.4"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "mongoose": "^6.0.2",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^7.0.0"
  },
  "lint-staged": {
    "**/*.{ts,json}": []
  }
}
